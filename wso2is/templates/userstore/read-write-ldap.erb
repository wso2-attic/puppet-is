<UserStoreManager class="org.wso2.carbon.user.core.ldap.ReadWriteLDAPUserStoreManager">
  <Property name="TenantManager">org.wso2.carbon.user.core.tenant.CommonHybridLDAPTenantManager</Property>
  <Property name="ConnectionURL"><%= @user_store['user_store_properties']['read-write-ldap']['connection_url'] %></Property>
  <Property name="ConnectionName"><%= @user_store['user_store_properties']['read-write-ldap']['connection_name'] %></Property>
  <Property name="ConnectionPassword"><%= @user_store['user_store_properties']['read-write-ldap']['connection_password'] %></Property>
  <Property name="AnonymousBind"><%= @user_store['user_store_properties']['read-write-ldap']['anonymous_bind'] %></Property>
  <Property name="UserSearchBase"><%= @user_store['user_store_properties']['read-write-ldap']['user_search_base'] %></Property>
  <Property name="UserNameAttribute"><%= @user_store['user_store_properties']['read-write-ldap']['username_attribute'] %></Property>
  <Property name="UserNameSearchFilter"><%= @user_store['user_store_properties']['read-write-ldap']['username_search_filter'] %></Property>
  <Property name="UserNameListFilter"><%= @user_store['user_store_properties']['read-write-ldap']['username_list_filter'] %></Property>
  <Property name="ReadGroups"><%= @user_store['user_store_properties']['read-write-ldap']['read_groups'] %></Property>
  <Property name="WriteGroups"><%= @user_store['user_store_properties']['read-write-ldap']['write_groups'] %></Property>
  <Property name="GroupSearchBase"><%= @user_store['user_store_properties']['read-write-ldap']['group_search_base'] %></Property>
  <Property name="GroupEntryObjectClass">groupOfNames</Property>
  <Property name="GroupNameAttribute"><%= @user_store['user_store_properties']['read-write-ldap']['groupname_attribute'] %></Property>
  <Property name="GroupNameSearchFilter"><%= @user_store['user_store_properties']['read-write-ldap']['groupname_search_filter'] %></Property>
  <Property name="GroupNameListFilter"><%= @user_store['user_store_properties']['read-write-ldap']['groupname_list_filter'] %></Property>
  <Property name="MembershipAttribute"><%= @user_store['user_store_properties']['read-write-ldap']['membership_attribute'] %></Property>
  <Property name="BackLinksEnabled"><%= @user_store['user_store_properties']['read-write-ldap']['back_links_enabled'] %></Property>
  <Property name="UsernameJavaRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['username_java_regex'] %></Property>
  <Property name="UsernameJavaScriptRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['username_javascript_regex'] %></Property>
  <Property name="UsernameJavaRegExViolationErrorMsg">Username pattern policy violated</Property>
  <Property name="PasswordJavaRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['password_javaregex'] %></Property>
  <Property name="PasswordJavaScriptRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['password_javascript_regex'] %></Property>
  <Property name="PasswordJavaRegExViolationErrorMsg">Password length should be within 5 to 30 characters</Property>
  <Property name="RolenameJavaRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['rolename_java_regex'] %></Property>
  <Property name="RolenameJavaScriptRegEx"><%= @user_store['user_store_properties']['read-write-ldap']['rolename_javascript_regex'] %></Property>
  <Property name="SCIMEnabled"><%= @user_store['user_store_properties']['read-write-ldap']['scim_enabled'] %></Property>
  <Property name="IsBulkImportSupported"><%= @user_store['user_store_properties']['read-write-ldap']['is_bulk_import_supported'] %></Property>
  <Property name="EmptyRolesAllowed"><%= @user_store['user_store_properties']['read-write-ldap']['empty_roles_allowed'] %></Property>
  <Property name="PasswordHashMethod"><%= @user_store['user_store_properties']['read-write-ldap']['password_hash_method'] %></Property>
  <Property name="MultiAttributeSeparator"><%= @user_store['user_store_properties']['read-write-ldap']['multi_attribute_separator'] %></Property>
  <Property name="MaxUserNameListLength"><%= @user_store['user_store_properties']['read-write-ldap']['max_username_list_length'] %></Property>
  <Property name="MaxRoleNameListLength"><%= @user_store['user_store_properties']['read-write-ldap']['max_rolename_list_length'] %></Property>
  <Property name="kdcEnabled"><%= @user_store['user_store_properties']['read-write-ldap']['kdc_enabled'] %></Property>
  <Property name="UserRolesCacheEnabled"><%= @user_store['user_store_properties']['read-write-ldap']['user_roles_cache_enabled'] %></Property>
  <Property name="ConnectionPoolingEnabled"><%= @user_store['user_store_properties']['read-write-ldap']['connection_pooling_enabled'] %></Property>
  <Property name="LDAPConnectionTimeout"><%= @user_store['user_store_properties']['read-write-ldap']['ldap_connection_timeout'] %></Property>
</UserStoreManager>